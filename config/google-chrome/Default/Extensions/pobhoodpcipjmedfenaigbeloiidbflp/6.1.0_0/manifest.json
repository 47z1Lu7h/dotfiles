{
   "action": {
      "default_icon": {
         "16": "images/MinimalTwitterIcon16.png",
         "32": "images/MinimalTwitterIcon32.png",
         "48": "images/MinimalTwitterIcon48.png"
      },
      "default_popup": "index.html",
      "default_title": "Minimal Twitter"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "js": [ "dist/main.js" ],
      "matches": [ "https://twitter.com/*", "https://mobile.twitter.com/*", "https://x.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Refine and declutter the ùïè/Twitter web experience.",
   "icons": {
      "128": "images/MinimalTwitterIcon128.png",
      "16": "images/MinimalTwitterIcon16.png",
      "32": "images/MinimalTwitterIcon32.png",
      "48": "images/MinimalTwitterIcon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Minimal Theme for Twitter / X",
   "options_ui": {
      "open_in_tab": true,
      "page": "index.html"
   },
   "permissions": [ "storage" ],
   "short_name": "Minimal Twitter",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "6.1.0",
   "web_accessible_resources": [ {
      "matches": [ "https://twitter.com/*", "https://mobile.twitter.com/*", "https://x.com/*" ],
      "resources": [ "css/main.css", "css/typefully.css", "fonts/inter-subset.woff2", "https://cdn.jsdelivr.net/gh/typefully/minimal-twitter@5.1/css/main.css", "https://cdn.jsdelivr.net/gh/typefully/minimal-twitter@5.1/css/typefully.css" ]
   } ]
}
