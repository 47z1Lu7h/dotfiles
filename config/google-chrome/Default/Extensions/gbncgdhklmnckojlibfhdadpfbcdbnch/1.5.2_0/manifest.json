   }, {
      "js": [ "script_document_start.js" ],
      "matches": [ "https://claude.ai/*", "https://www.bing.com/chat*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "host_permissions": [ "https://chatgpt.com/*", "https://poe.com/*", "https://www.phind.com/*", "https://chat.mistral.ai/*", "https://www.chatpdf.com/*", "https://www.perplexity.ai/*", "https://claude.ai/*", "https://www.bing.com/chat*", "https://you.com/*", "https://dashboard.cohere.com/playground/chat" ],
